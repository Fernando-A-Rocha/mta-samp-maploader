 <meta>
   <!-- DOCUMENTATION: https://github.com/Fernando-A-Rocha/mta-samp-maploader#readme -->
   <info author="Fernando"
    name="SAMP Map Loader"
    description="parses and loads SA-MP Pawn maps"
    version="1.0.0"
    type="script" />

   <!-- because of engineGetModelTextures -->
   <min_mta_version client="1.5.7-9.20416"></min_mta_version>
  
   <!-- Required dependencies -->
   <include resource="newmodels" />
   <include resource="sampobj_reloaded" />
   
   <!-- SA-MP Map Models -->
      <!-- models used by 'office' map -->
      <file src="models/bigtire.dff"/>
      <file src="models/bigtire.txd"/>
      <file src="models/bigtire.col"/>
      <file src="models/celltoilet.dff"/>
      <file src="models/celltoilet.txd"/>
      <file src="models/celltoilet.col"/>
      <file src="models/money.dff"/>
      <file src="models/money.txd"/>
      <file src="models/money.col"/>
      <file src="models/wallclock.dff"/>
      <file src="models/wallclock.txd"/>
      <file src="models/wallclock.col"/>

   <!-- SA-MP Maps -->
      <file src="maps/mansion.pwn"/>
      <file src="maps/office.pwn"/>
      <file src="maps/gunshop.pwn"/>
      <file src="maps/prison1.pwn"/>
      <file src="maps/prison2.pwn"/>
      <file src="maps/ext_ElCoronaTest.pwn"/>

   <!-- Main Scripts -->
      <script type="shared" src="_config.lua"/>
      <script type="client" src="async.lua"/>
      <script type="client" src="utility.lua"/>
      <script type="shared" src="functions_shared.lua"/>
      <script type="client" src="functions_client.lua"/>
      <script type="server" src="functions_server.lua"/>
      <script type="shared" src="map_list.lua"/>
      <script type="client" src="client.lua"/>
      <script type="server" src="server.lua"/>   

   <!-- shaders -->
     <file src="files/mta-helper.fx"/>
     <file src="files/shader.fx"/>
     <file src="files/mat_color.fx"/>
</meta>
